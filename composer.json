{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "artesaos/seotools": "^0.20.1",
        "benjamincrozat/laravel-dropbox-driver": "^3.0",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "ghasedak/laravel": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hekmatinasser/verta": "^1.11",
        "laravel/framework": "^8.12",
        "laravel/sanctum": "^2.6",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.0",
        "obiefy/api-response": "^0.9.7",
        "spatie/laravel-backup": "^7.6",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^3.18",
        "spatie/laravel-sitemap": "^6.0",
        "tcg/voyager": "^1.4",
        "ext-intl": "*",
      "ext-curl": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.3",
        "mdhesari/mysql-localhost": "dev-main",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope",
                "ghasedak/laravel"
            ]
        }
    },
    "autoload": {
        "files": [
            "app/Space/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Leantony\\Grid\\": "Packages/Leantony/Grid/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
